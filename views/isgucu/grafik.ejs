<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Grafik</title>
    <!-- Chart.js kütüphanesi -->
    <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
</head>
<body>
    <canvas id="myChart" width="400" height="400"></canvas>

    <script>
        
        // Grafik verilerini alma
        const data = JSON.parse(`<%- data %>`); // Değişiklik burada

        // Canvas elementini seçme
        const ctx = document.getElementById('myChart').getContext('2d');

        // Grafik oluşturma
        new Chart(ctx, {
            type: 'line',
            data: data,
        });
    </script>
</body>
</html>
